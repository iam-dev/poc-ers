{
  "printWidth": 120,  
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "overrides": [
      {
        "files": "*.sol",
        "options": {
          "printWidth": 120,
          "tabWidth": 4,
          "useTabs": false,
          "bracketSpacing": true,
          "singleQuote": false,
          "explicitTypes": "always"
        }
      }
    ]
}